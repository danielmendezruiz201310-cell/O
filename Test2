local Panels={}
for tabName,controls in pairs(menuTabs)do
local tabPanel=Instance.new("Frame",HubFrame)
tabPanel.BackgroundTransparency=1
tabPanel.Size=UDim2.new(1,-38,1,-70)
tabPanel.Position=UDim2.new(0,29,0,58)
tabPanel.Visible=tabName=="Reach"
Panels[tabName]=tabPanel
local layout=Instance.new("UIListLayout",tabPanel)layout.Padding=UDim.new(0,12)
for _,ctrl in ipairs(controls)do
local row=Instance.new("Frame",tabPanel)
row.Size=UDim2.new(1,0,0,52)row.BackgroundColor3=Color3.fromRGB(unpack(settings.theme.PanelActive))row.BackgroundTransparency=0.20
Instance.new("UICorner",row)
local icon=Instance.new("ImageLabel",row)icon.Image=ctrl.icon icon.BackgroundTransparency=1 icon.Size=UDim2.fromOffset(24,24)icon.Position=UDim2.new(0,12,0.5,-12)
local label=Instance.new("TextLabel",row)label.Text=ctrl.label label.BackgroundTransparency=1 label.TextColor3=Color3.fromRGB(unpack(settings.theme.Text))label.Font=Enum.Font.Gotham label.TextSize=20 label.Size=UDim2.new(0.5,0,1,0)label.Position=UDim2.new(0,48,0,0)label.TextXAlignment=Enum.TextXAlignment.Left
if ctrl.type=="toggle"then
local tBtn=Instance.new("TextButton",row)
tBtn.Size=UDim2.fromOffset(44,32)
tBtn.Position=UDim2.new(1,-52,0.5,-16)
tBtn.AnchorPoint=Vector2.new(1,0.5)
tBtn.BackgroundColor3=settings[ctrl.key]and Color3.fromRGB(unpack(settings.theme.Accent))or Color3.fromRGB(84,12,104)
tBtn.Text=settings[ctrl.key]and"ON"or"OFF"
tBtn.TextColor3=Color3.new(1,1,1)
tBtn.Font=Enum.Font.GothamBold tBtn.TextSize=18 Instance.new("UICorner",tBtn).CornerRadius=UDim.new(0,9)
tBtn.MouseButton1Click:Connect(function()settings[ctrl.key]=not settings[ctrl.key]tBtn.Text=settings[ctrl.key]and"ON"or"OFF"tBtn.BackgroundColor3=settings[ctrl.key]and Color3.fromRGB(unpack(settings.theme.Accent))or Color3.fromRGB(84,12,104)saveSettings()end)
elseif ctrl.type=="slider"then
local slider=Instance.new("Frame",row)slider.Size=UDim2.new(0.43,0,0,12)slider.Position=UDim2.new(1,-138,0.5,-6)slider.BackgroundColor3=Color3.fromRGB(44,77,220)Instance.new("UICorner",slider).CornerRadius=UDim.new(0,6)
local fill=Instance.new("Frame",slider)fill.Size=UDim2.new((settings[ctrl.key]-ctrl.min)/(ctrl.max-ctrl.min),0,1,0)fill.BackgroundColor3=Color3.fromRGB(unpack(settings.theme.Accent))Instance.new("UICorner",fill).CornerRadius=UDim.new(0,8)
local knob=Instance.new("ImageButton",slider)knob.Size=UDim2.fromOffset(18,18)knob.Position=UDim2.new((settings[ctrl.key]-ctrl.min)/(ctrl.max-ctrl.min),-9,0,-3)knob.AnchorPoint=Vector2.new(0,0.5)knob.Image=assets.triangle knob.BackgroundTransparency=1
knob.MouseButton1Down:Connect(function()
local moveConn moveConn=UserInputService.InputChanged:Connect(function(input)if input.UserInputType==Enum.UserInputType.MouseMovement then local rel=math.clamp((input.Position.X-slider.AbsolutePosition.X)/slider.AbsoluteSize.X,0,1)local val=ctrl.min+(ctrl.max-ctrl.min)*rel settings[ctrl.key]=math.round(val)fill.Size=UDim2.new(rel,0,1,0)knob.Position=UDim2.new(rel,-9,0,-3)saveSettings()end end)
UserInputService.InputEnded:Connect(function(input)if input.UserInputType==Enum.UserInputType.MouseButton1 then moveConn:Disconnect()end end)
end)
end
end
end

for idx,sBtn in pairs(sidebarBtnObjs)do
sBtn.MouseButton1Click:Connect(function()
for tab,pan in pairs(Panels)do pan.Visible=false end
Panels[sidebarBtns[idx][2]].Visible=true
for i,btn in pairs(sidebarBtnObjs)do btn.BackgroundColor3=i==idx and Color3.fromRGB(unpack(settings.theme.PanelActive))or Color3.fromRGB(unpack(settings.theme.Panel))end
end)
end

LoginBtn.MouseButton1Click:Connect(function()
clickSound:Play()
local user=UsernameBox.Text
local key=KeyBox.Text
if key=="free2025"and user==player.Name then
saveUser(user)
TweenService:Create(LoginFrame,TweenInfo.new(0.8,Enum.EasingStyle.Quint),{Size=UDim2.fromOffset(480,400),BackgroundTransparency=1}):Play()
TweenService:Create(Background,TweenInfo.new(1),{ImageTransparency=1}):Play()
task.wait(0.7)
LoginFrame.Visible=false
Background.Visible=false
HubFrame.Visible=true
Sidebar.Visible=true
else
KeyBox.Text=""
KeyBox.PlaceholderText="Invalid Key"
TweenService:Create(KeyBox,TweenInfo.new(0.3),{TextColor3=Color3.fromRGB(255,100,100)}):Play()
task.wait(0.4)
TweenService:Create(KeyBox,TweenInfo.new(0.3),{TextColor3=Color3.fromRGB(230,240,255)}):Play()
KeyBox.PlaceholderText="Key"
end
end)

local Camera=Workspace.CurrentCamera
local char=player.Character or player.CharacterAdded:Wait()
local hum=char:WaitForChild("Humanoid")
local hrp=char:WaitForChild("HumanoidRootPart")
player.CharacterAdded:Connect(function(c)char=c hum=c:WaitForChild("Humanoid")hrp=c:WaitForChild("HumanoidRootPart")end)

local function getBalls()local balls={}pcall(function()for _,b in CollectionService:GetTagged("Ball")do if b:IsA("BasePart")then table.insert(balls,b)end end end)return balls end

local hitbox=Instance.new("Part")
hitbox.Name="NuluxReachBox"
hitbox.Anchored=true
hitbox.CanCollide=false
hitbox.Transparency=1
hitbox.Material=Enum.Material.ForceField
hitbox.Parent=Workspace

local box=Instance.new("SelectionBox")box.Adornee=hitbox box.LineThickness=0.05 box.Color3=Color3.fromRGB(0,255,255)box.Transparency=0.5 box.Parent=hitbox

local params=OverlapParams.new()params.FilterType=Enum.RaycastFilterType.Blacklist params.FilterDescendantsInstances={hitbox}

local lastKick,kickPlaying=0,false
hum.AnimationPlayed:Connect(function(anim)if anim.Animation.AnimationId:find("kick")or anim.Animation.AnimationId:find("shot")then kickPlaying=true anim.Stopped:Connect(function()kickPlaying=false end)end end)

RunService.Heartbeat:Connect(function()
local balls=getBalls()
if #balls==0 or not hrp then return end
local showVis=settings.visOn or settings.reachOn
if showVis then
hitbox.CFrame=hrp.CFrame*CFrame.new(settings.offsetX,settings.offsetY,settings.offsetZ)
hitbox.Size=Vector3.new(settings.reachX,settings.reachY,settings.reachZ)
box.Transparency=math.sin(tick()*4)*0.3+0.4
else
box.Transparency=1
end
if settings.reachOn then
local parts=Workspace:GetPartBoundsInBox(hitbox.CFrame,hitbox.Size,params)
for _,part in parts do
for _,ball in balls do
if part==ball then
local limbs={"LeftFoot","RightFoot","Left Leg","Right Leg","LeftHand","RightHand"}
for _,limbName in limbs do
local limb=char:FindFirstChild(limbName)
if limb then for i=1,8 do firetouchinterest(limb,ball,0)firetouchinterest(limb,ball,1)end end
end
end
end
end
end
if settings.infStamOn then
spawn(function()
while settings.infStamOn do
pcall(function()
local ps=player:FindFirstChild("PlayerScripts")
if ps then
local c=ps:FindFirstChild("controllers")
if c then
local m=c:FindFirstChild("movementController")
if m then
local s=m:FindFirstChild("stamina")
if s then s.Value=100 end
end
end
end
end)
task.wait(0.2)
end
end)
end
if settings.alwaysSprintOn then hum.WalkSpeed=30 end
if settings.powershotOn and kickPlaying then local ball=balls[1]if ball then ball.AssemblyLinearVelocity=ball.AssemblyLinearVelocity*1.8 end end
if settings.autoTopBinsOn and kickPlaying and tick()-lastKick>1 then
lastKick=tick()
local oppositeZ=hrp.Position.Z>0 and -53 or 53
local ball=balls[1]
if ball then
ball.AssemblyLinearVelocity=(Vector3.new(math.random(-3,3),math.random(4,5.5),oppositeZ)-ball.Position).Unit*170+Vector3.new(0,25,0)
end
end
if settings.bringBallOn then local ball=balls[1]if ball then ball.AssemblyLinearVelocity=ball.AssemblyLinearVelocity+(hrp.Position-ball.Position).Unit*80 end end
if settings.autoGKReactOn then hrp.AssemblyLinearVelocity=hrp.AssemblyLinearVelocity+hrp.CFrame.LookVector*settings.diveDist end
if settings.antiRagdollOn then hum:SetStateEnabled(Enum.HumanoidStateType.Ragdoll,false) end
if settings.autoCatchOn then local ball=balls[1]if ball and (ball.Position-hrp.Position).Magnitude<12 then ball.AssemblyLinearVelocity=(hrp.Position-ball.Position).Unit*120 end end
end)
