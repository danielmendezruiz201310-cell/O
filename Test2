local RunService=game:GetService("RunService")
local SoundService=game:GetService("SoundService")
local player=Players.LocalPlayer

local SETTING_FILE="NuluxSettings.json"
local USER_FILE="NuluxUser.json"
local defaultSettings={reachOn=false,visOn=false,reachX=30,reachY=30,reachZ=30,offsetX=0,offsetY=0,offsetZ=0,predOn=false,lineThick=1.5,advPred=false,infStamOn=false,alwaysSprintOn=false,antiRagdollOn=false,autoCatchOn=false,autoGKReactOn=false,noGKCooldownOn=false,diveDist=120,powershotOn=false,infShotChargeOn=false,autoTopBinsOn=false,bringBallOn=false,autoGoalFixed=false,theme={Accent={44,77,220},Background={20,22,40},Panel={24,23,32},PanelActive={34,35,68},Text={230,240,255}}}
local settings=defaultSettings

pcall(function()if isfile and isfile(SETTING_FILE)then settings=HttpService:JSONDecode(readfile(SETTING_FILE))end end)
pcall(function()if isfile and isfile(USER_FILE)then savedUser=HttpService:JSONDecode(readfile(USER_FILE))end end)

local function saveSettings()pcall(function()if writefile then writefile(SETTING_FILE,HttpService:JSONEncode(settings))end end)end
local function saveUser(userId)pcall(function()if writefile then writefile(USER_FILE,HttpService:JSONEncode({userId=userId}))end end)end

