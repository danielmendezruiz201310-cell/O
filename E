local Fluent=loadstring(game:HttpGet("https://github.com/ActualMasterOogway/Fluent-Renewed/releases/latest/download/Fluent.luau"))()

local Window=Fluent:CreateWindow({
Title="Nulux Hub ðŸŽ„",
TabWidth=160,
Size=UDim2.fromOffset(580,400),
Theme="Aqua"
})

local Players=game:GetService("Players")
local RunService=game:GetService("RunService")
local CollectionService=game:GetService("CollectionService")
local Workspace=game:GetService("Workspace")
local HttpService=game:GetService("HttpService")
local player=Players.LocalPlayer
local Camera=Workspace.CurrentCamera

local saveFile="NuluxMinimal.json"

local defaultSettings={
reachOn=false,visOn=false,
reachX=30,reachY=30,reachZ=30,
offsetX=0,offsetY=0,offsetZ=0,
predOn=false,lineThick=1.5,advPred=false
}

local settings=defaultSettings

pcall(function()
if isfile and isfile(saveFile)then
settings=HttpService:JSONDecode(readfile(saveFile))
end
end)

local function saveSettings()
pcall(function()
if writefile then writefile(saveFile,HttpService:JSONEncode(settings))end
end)
end

local Tab=Window:AddTab({Title="Main"})

Tab:AddToggle("Reach Enabled",{Default=settings.reachOn,Callback=function(v)settings.reachOn=v saveSettings()end})
Tab:AddToggle("Visualizer",{Default=settings.visOn,Callback=function(v)settings.visOn=v saveSettings()end})

Tab:AddSlider("Reach X",{Min=1,Max=150,Default=settings.reachX,Rounding=0,Callback=function(v)settings.reachX=v saveSettings()end})
Tab:AddSlider("Reach Y",{Min=1,Max=150,Default=settings.reachY,Rounding=0,Callback=function(v)settings.reachY=v saveSettings()end})
Tab:AddSlider("Reach Z",{Min=1,Max=150,Default=settings.reachZ,Rounding=0,Callback=function(v)settings.reachZ=v saveSettings()end})
Tab:AddSlider("Offset X",{Min=-50,Max=50,Default=settings.offsetX,Rounding=0,Callback=function(v)settings.offsetX=v saveSettings()end})
Tab:AddSlider("Offset Y",{Min=-50,Max=50,Default=settings.offsetY,Rounding=0,Callback=function(v)settings.offsetY=v saveSettings()end})
Tab:AddSlider("Offset Z",{Min=-50,Max=50,Default=settings.offsetZ,Rounding=0,Callback=function(v)settings.offsetZ=v saveSettings()end})

Tab:AddToggle("Ball Predictor",{Default=settings.predOn,Callback=function(v)settings.predOn=v saveSettings()end})
Tab:AddSlider("Line Thickness",{Min=0.2,Max=10,Default=settings.lineThick,Rounding=1,Callback=function(v)settings.lineThick=v saveSettings()end})
Tab:AddToggle("Advanced Predictor",{Default=settings.advPred,Callback=function(v)settings.advPred=v saveSettings()end})

local char=player.Character or player.CharacterAdded:Wait()
local hrp=char:WaitForChild("HumanoidRootPart")

player.CharacterAdded:Connect(function(c)char=c hrp=c:WaitForChild("HumanoidRootPart")end)

local function getBalls()
local balls={}
pcall(function()
for _,b in CollectionService:GetTagged("Ball")do if b:IsA("BasePart")then table.insert(balls,b)end end
end)
return balls
end

local hitbox=Instance.new("Part")
hitbox.Name="NuluxReachBox"
hitbox.Anchored=true
hitbox.CanCollide=false
hitbox.Transparency=1
hitbox.Material=Enum.Material.ForceField
hitbox.Parent=Workspace

local box=Instance.new("SelectionBox")
box.Adornee=hitbox
box.LineThickness=0.05
box.Color3=Color3.fromRGB(0,255,255)
box.Transparency=0.5
box.Parent=hitbox

local grad=Instance.new("UIGradient")
grad.Color=ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(0,255,255)),ColorSequenceKeypoint.new(1,Color3.fromRGB(100,255,255))}
grad.Rotation=45
grad.Parent=box

local stroke=Instance.new("UIStroke")
stroke.Color=Color3.fromRGB(0,255,255)
stroke.Thickness=3
stroke.Transparency=0.2
stroke.Parent=box

local particles=Instance.new("ParticleEmitter")
particles.Texture="rbxassetid://243660364"
particles.Lifetime=NumberRange.new(0.4)
particles.Rate=40
particles.Speed=NumberRange.new(2)
particles.Color=ColorSequence.new(Color3.fromRGB(0,255,255))
particles.Transparency=NumberSequence.new(0.4,1)
particles.LightEmission=1
particles.Parent=hitbox

local predLine
local dot=Drawing.new("Circle")
dot.Visible=false
dot.Thickness=4
dot.Color=Color3.fromRGB(255,0,0)
dot.Filled=true
dot.Radius=12

local timeLabel=Drawing.new("Text")
timeLabel.Visible=false
timeLabel.Size=24
timeLabel.Color=Color3.fromRGB(255,50,50)
timeLabel.Center=true
timeLabel.Outline=true

local params=OverlapParams.new()
params.FilterType=Enum.RaycastFilterType.Blacklist
params.FilterDescendantsInstances={hitbox}

RunService.Heartbeat:Connect(function()
local balls=getBalls()
if #balls==0 or not hrp then return end

local effectiveReach=settings.reachOn
local showVis=settings.visOn or effectiveReach

if showVis then
hitbox.CFrame=hrp.CFrame*CFrame.new(settings.offsetX,settings.offsetY,settings.offsetZ)
hitbox.Size=Vector3.new(settings.reachX,settings.reachY,settings.reachZ)
box.Transparency=math.sin(tick()*4)*0.3+0.4
stroke.Transparency=0.1
particles.Enabled=true
else
box.Transparency=1
stroke.Transparency=1
particles.Enabled=false
end

if effectiveReach then
local parts=Workspace:GetPartBoundsInBox(hitbox.CFrame,hitbox.Size,params)
for _,part in parts do
for _,ball in balls do
if part==ball then
local limbs={"LeftFoot","RightFoot","Left Leg","Right Leg","LeftHand","RightHand"}
for _,limbName in limbs do
local limb=char:FindFirstChild(limbName)
if limb then
for i=1,8 do
firetouchinterest(limb,ball,0)
firetouchinterest(limb,ball,1)
end
end
end
end
end
end
end

if settings.predOn then
local ball=balls[1]
if ball and ball.AssemblyLinearVelocity.Magnitude>5 then
predLine=Workspace:FindFirstChild("NuluxPred")or Instance.new("Part")
predLine.Name="NuluxPred"
predLine.Anchored=true
predLine.CanCollide=false
predLine.Material=Enum.Material.Neon
predLine.Color=Color3.fromRGB(0,255,255)
predLine.Parent=Workspace
local dir=ball.AssemblyLinearVelocity.Unit*150
predLine.Size=Vector3.new(settings.lineThick,settings.lineThick,dir.Magnitude)
predLine.CFrame=CFrame.new(ball.Position,ball.Position+dir)*CFrame.new(0,0,-dir.Magnitude/2)
if settings.advPred then
local future=ball.Position+ball.AssemblyLinearVelocity*0.4
local pos,on=Camera:WorldToViewportPoint(future)
if on then
dot.Position=Vector2.new(pos.X,pos.Y)
dot.Visible=true
timeLabel.Position=Vector2.new(pos.X,pos.Y+25)
timeLabel.Text=string.format("%.2fs",(hrp.Position-future).Magnitude/ball.AssemblyLinearVelocity.Magnitude)
timeLabel.Visible=true
else
dot.Visible=false
timeLabel.Visible=false
end
end
elseif predLine then predLine:Destroy() end
elseif predLine then predLine:Destroy() dot.Visible=false timeLabel.Visible=false end
end)

Fluent:Notify({Title="Nulux Hub ðŸŽ„."})
