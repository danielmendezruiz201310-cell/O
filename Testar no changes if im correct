		createSlider(ScrollFrame, "Offset Y", "gkOffsetY", -50, 50, 1, 8)
		createSlider(ScrollFrame, "Offset Z", "gkOffsetZ", -50, 50, 1, 9)
		createSlider(ScrollFrame, "Box Opacity", "gkBoxOpacity", 0, 1, 0.1, 10)
		createColorPicker(ScrollFrame, "Box Color", "gkBoxColor", 11)
	elseif currentTab == "Settings" then
		createHeader(ScrollFrame, "UI SETTINGS", 1)
		local hotkeyFrame = Instance.new("Frame")
		hotkeyFrame.Size = UDim2.new(1, 0, 0, 52)
		hotkeyFrame.BackgroundColor3 = Color3.fromRGB(39, 39, 42)
		hotkeyFrame.LayoutOrder = 2
		hotkeyFrame.Parent = ScrollFrame
		local hotkeyCorner = Instance.new("UICorner", hotkeyFrame)
		hotkeyCorner.CornerRadius = UDim.new(0, 8)
		local hotkeyTitle = Instance.new("TextLabel")
		hotkeyTitle.Size = UDim2.new(1, -32, 0, 20)
		hotkeyTitle.Position = UDim2.new(0, 16, 0, 8)
		hotkeyTitle.BackgroundTransparency = 1
		hotkeyTitle.Text = "Toggle UI Key"
		hotkeyTitle.TextColor3 = Color3.fromRGB(212, 212, 216)
		hotkeyTitle.Font = Enum.Font.Gotham
		hotkeyTitle.TextSize = 14
		hotkeyTitle.TextXAlignment = Enum.TextXAlignment.Left
		hotkeyTitle.Parent = hotkeyFrame
		local bindButton = Instance.new("TextButton")
		bindButton.Size = UDim2.new(1, -32, 0, 24)
		bindButton.Position = UDim2.new(0, 16, 0, 28)
		bindButton.BackgroundColor3 = Color3.fromRGB(63, 63, 70)
		bindButton.Text = _G.settings.hotkey.Name
		bindButton.TextColor3 = Color3.fromRGB(212, 212, 216)
		bindButton.Font = Enum.Font.GothamBold
		bindButton.TextSize = 14
		bindButton.AutoButtonColor = false
		bindButton.Parent = hotkeyFrame
		local bindCorner = Instance.new("UICorner", bindButton)
		bindCorner.CornerRadius = UDim.new(0, 6)
		local isBinding = false
		local bindConnection
		bindButton.MouseButton1Click:Connect(function()
			if isBinding then return end
			isBinding = true
			bindButton.Text = "Press any key..."
			bindButton.BackgroundColor3 = Color3.fromRGB(0, 255, 255)
			bindButton.TextColor3 = Color3.fromRGB(0, 0, 0)
			bindConnection = UserInputService.InputBegan:Connect(function(input, gameProcessed)
				if gameProcessed then return end
				if input.UserInputType == Enum.UserInputType.Keyboard then
					_G.settings.hotkey = input.KeyCode
					bindButton.Text = input.KeyCode.Name
					bindButton.BackgroundColor3 = Color3.fromRGB(63, 63, 70)
					bindButton.TextColor3 = Color3.fromRGB(212, 212, 216)
					isBinding = false
					if bindConnection then
						bindConnection:Disconnect()
						bindConnection = nil
					end
				end
			end)
			task.delay(5, function()
				if isBinding then
					bindButton.Text = _G.settings.hotkey.Name
					bindButton.BackgroundColor3 = Color3.fromRGB(63, 63, 70)
					bindButton.TextColor3 = Color3.fromRGB(212, 212, 216)
					isBinding = false
					if bindConnection then
						bindConnection:Disconnect()
						bindConnection = nil
					end
				end
			end)
		end)
	elseif currentTab == "Discord" then
		createHeader(ScrollFrame, "JOIN OUR COMMUNITY", 1)
		local discordFrame = Instance.new("Frame")
		discordFrame.Size = UDim2.new(1, 0, 0, 100)
		discordFrame.BackgroundColor3 = Color3.fromRGB(88, 101, 242)
