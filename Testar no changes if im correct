		discordCorner.CornerRadius = UDim.new(0, 8)
		local discordTitle = Instance.new("TextLabel")
		discordTitle.Size = UDim2.new(1, -24, 0, 24)
		discordTitle.Position = UDim2.new(0, 12, 0, 20)
		discordTitle.BackgroundTransparency = 1
		discordTitle.Text = "Join RF24 Discord!"
		discordTitle.TextColor3 = Color3.new(1, 1, 1)
		discordTitle.Font = Enum.Font.GothamBold
		discordTitle.TextSize = 16
		discordTitle.Parent = discordFrame
		local joinButton = Instance.new("TextButton")
		joinButton.Size = UDim2.new(1, -24, 0, 40)
		joinButton.Position = UDim2.new(0, 12, 0, 52)
		joinButton.BackgroundColor3 = Color3.new(1, 1, 1)
		joinButton.Text = "Copy Discord Link"
		joinButton.TextColor3 = Color3.fromRGB(88, 101, 242)
		joinButton.Font = Enum.Font.GothamBold
		joinButton.TextSize = 15
		joinButton.AutoButtonColor = false
		joinButton.Parent = discordFrame
		local joinCorner = Instance.new("UICorner", joinButton)
		joinCorner.CornerRadius = UDim.new(0, 10)
		joinButton.MouseButton1Click:Connect(function()
			pcall(function()
				setclipboard("https://discord.gg/rUB6rAaP")
			end)
			joinButton.Text = "Link Copied!"
			TweenService:Create(joinButton, TweenInfo.new(0.3), {
				BackgroundColor3 = Color3.fromRGB(67, 181, 129)
			}):Play()
			task.delay(3, function()
				joinButton.Text = "Copy Discord Link"
				TweenService:Create(joinButton, TweenInfo.new(0.5), {
					BackgroundColor3 = Color3.new(1, 1, 1)
				}):Play()
			end)
		end)
	elseif currentTab == "Credits" then
		createHeader(ScrollFrame, "CREDITS", 1)
		local creditsFrame = Instance.new("Frame")
		creditsFrame.Size = UDim2.new(1, 0, 0, 100)
		creditsFrame.BackgroundColor3 = Color3.fromRGB(39, 39, 42)
		creditsFrame.Parent = ScrollFrame
		local creditsCorner = Instance.new("UICorner", creditsFrame)
		creditsCorner.CornerRadius = UDim.new(0, 8)
		local creditsTitle = Instance.new("TextLabel")
		creditsTitle.Size = UDim2.new(1, -20, 0, 18)
		creditsTitle.Position = UDim2.new(0, 10, 0, 10)
		creditsTitle.BackgroundTransparency = 1
		creditsTitle.Text = "RF24 Privat - Full Featured"
		creditsTitle.TextColor3 = Color3.fromRGB(0, 255, 255)
		creditsTitle.Font = Enum.Font.GothamBold
		creditsTitle.TextSize = 15
		creditsTitle.TextXAlignment = Enum.TextXAlignment.Left
		creditsTitle.Parent = creditsFrame
		local creditsDesc = Instance.new("TextLabel")
		creditsDesc.Size = UDim2.new(1, -20, 0, 80)
		creditsDesc.Position = UDim2.new(0, 10, 0, 28)
		creditsDesc.BackgroundTransparency = 1
		creditsDesc.Text = "• Reach System with Visualizer\n• Ball Predictor\n• Movement Boosts\n• Auto Goal\n• GK Features"
		creditsDesc.TextColor3 = Color3.fromRGB(113, 113, 122)
		creditsDesc.Font = Enum.Font.Gotham
		creditsDesc.TextSize = 13
		creditsDesc.TextXAlignment = Enum.TextXAlignment.Left
		creditsDesc.TextYAlignment = Enum.TextYAlignment.Top
		creditsDesc.Parent = creditsFrame
	end
end

RunService.Heartbeat:Connect(function()
	if _G.settings.reachOn and hrp then
		hitbox.CFrame = hrp.CFrame * CFrame.new(_G.settings.offsetX, _G.settings.offsetY, _G.settings.offsetZ)
		hitbox.Size = Vector3.new(_G.settings.reachX, _G.settings.reachY, _G.settings.reachZ)
		sel.Transparency = _G.settings.visOn and (1 - _G.settings.boxOpacity) or 1
		sel.Color3 = _G.settings.boxColor
		local parts = Workspace:GetPartBoundsInBox(hitbox.CFrame, hitbox.Size)
		for _,part in ipairs(parts) do
			if part ~= hitbox and part.Parent ~= char then
				local limbs = {"RightFoot","LeftFoot","Right Leg","Left Leg","RightHand","LeftHand","Head","UpperTorso","LowerTorso","HumanoidRootPart"}
				for _,limbName in ipairs(limbs) do
					local limb = char:FindFirstChild(limbName)
					if limb then
						firetouchinterest(limb, part, 0)
						firetouchinterest(limb, part, 1)
					end
				end
			end
		end
	else
		hitbox.Size = Vector3.new(0,0,0)
	end
	if _G.settings.bringBallOn then
		local ball = getBall()
		if ball and hrp then
			ball.CFrame = hrp.CFrame * CFrame.new(0, 2, -3)
			ball.AssemblyLinearVelocity = Vector3.new(0,0,0)
		end
	end
	if _G.settings.gkReachOn and hrp then
		gkHitbox.CFrame = hrp.CFrame * CFrame.new(_G.settings.gkOffsetX, _G.settings.gkOffsetY, _G.settings.gkOffsetZ)
		gkHitbox.Size = Vector3.new(_G.settings.gkReachX, _G.settings.gkReachY, _G.settings.gkReachZ)
		gkSel.Transparency = _G.settings.gkVisOn and (1 - _G.settings.gkBoxOpacity) or 1
		gkSel.Color3 = _G.settings.gkBoxColor
		local parts = Workspace:GetPartBoundsInBox(gkHitbox.CFrame, gkHitbox.Size)
		for _,part in ipairs(parts) do
			if part ~= gkHitbox and part ~= hitbox and part.Parent ~= char then
				local limbs = {"RightFoot","LeftFoot","Right Leg","Left Leg","RightHand","LeftHand","Head","UpperTorso","LowerTorso","HumanoidRootPart"}
				for _,limbName in ipairs(limbs) do
					local limb = char:FindFirstChild(limbName)
					if limb then
						firetouchinterest(limb, part, 0)
						firetouchinterest(limb, part, 1)
					end
				end
			end
		end
	else
		gkHitbox.Size = Vector3.new(0,0,0)
	end
end)

UserInputService.InputBegan:Connect(function(input, gp)
	if not gp and input.KeyCode == _G.settings.hotkey then
		ScreenGui.Enabled = not ScreenGui.Enabled
	end
end)

updateUI()
