    while LoadingFrame.Parent do
        TweenService:Create(LoadSpinner, TweenInfo.new(2, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1), {Rotation = 360}):Play()
        task.wait()
    end
end)
task.wait(2)
TweenService:Create(LoadingFrame, TweenInfo.new(1), {BackgroundTransparency = 1}):Play()
TweenService:Create(LoadingTitle, TweenInfo.new(1), {TextTransparency = 1}):Play()
TweenService:Create(LoadingSubtitle, TweenInfo.new(1), {TextTransparency = 1}):Play()
TweenService:Create(LoadSpinner, TweenInfo.new(1), {ImageTransparency = 1}):Play()
task.wait(1)
LoadingFrame:Destroy()
