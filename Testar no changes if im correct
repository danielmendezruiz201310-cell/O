local function createToggle(parent, title, key, order)
	local frame = Instance.new("Frame")
	frame.Size = UDim2.new(1, 0, 0, 40)
	frame.BackgroundColor3 = Color3.fromRGB(39, 39, 42)
	frame.LayoutOrder = order
	frame.Parent = parent
	local corner = Instance.new("UICorner", frame)
	corner.CornerRadius = UDim.new(0, 8)
	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(1, -80, 1, 0)
	label.Position = UDim2.new(0, 16, 0, 0)
	label.BackgroundTransparency = 1
	label.Text = title
	label.TextColor3 = Color3.fromRGB(212, 212, 216)
	label.Font = Enum.Font.Gotham
	label.TextSize = 14
	label.TextXAlignment = Enum.TextXAlignment.Left
	label.Parent = frame
	local toggleBg = Instance.new("Frame")
	toggleBg.Size = UDim2.fromOffset(44, 24)
	toggleBg.Position = UDim2.new(1, -60, 0.5, -12)
	toggleBg.BackgroundColor3 = _G.settings[key] and Color3.fromRGB(0, 255, 255) or Color3.fromRGB(63, 63, 70)
	toggleBg.Parent = frame
	local toggleCorner = Instance.new("UICorner", toggleBg)
	toggleCorner.CornerRadius = UDim.new(1, 0)
	local knob = Instance.new("Frame")
	knob.Size = UDim2.fromOffset(16, 16)
	knob.Position = _G.settings[key] and UDim2.new(0, 24, 0.5, -8) or UDim2.new(0, 4, 0.5, -8)
	knob.BackgroundColor3 = Color3.new(1,1,1)
	knob.Parent = toggleBg
	local knobCorner = Instance.new("UICorner", knob)
	knobCorner.CornerRadius = UDim.new(1, 0)
	local btn = Instance.new("TextButton")
	btn.Size = UDim2.new(1,0,1,0)
	btn.BackgroundTransparency = 1
	btn.Text = ""
	btn.Parent = frame
	btn.MouseButton1Click:Connect(function()
		_G.settings[key] = not _G.settings[key]
		TweenService:Create(toggleBg, TweenInfo.new(0.2), {
			BackgroundColor3 = _G.settings[key] and Color3.fromRGB(0, 255, 255) or Color3.fromRGB(63, 63, 70)
		}):Play()
		TweenService:Create(knob, TweenInfo.new(0.2), {
			Position = _G.settings[key] and UDim2.new(0, 24, 0.5, -8) or UDim2.new(0, 4, 0.5, -8)
		}):Play()
	end)
end

local function createSlider(parent, title, key, min, max, step, order)
	local frame = Instance.new("Frame")
	frame.Size = UDim2.new(1, 0, 0, 56)
	frame.BackgroundColor3 = Color3.fromRGB(39, 39, 42)
	frame.LayoutOrder = order
	frame.Parent = parent
	local corner = Instance.new("UICorner", frame)
	corner.CornerRadius = UDim.new(0, 8)
	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(0.6, 0, 0, 18)
	label.Position = UDim2.new(0, 16, 0, 8)
	label.BackgroundTransparency = 1
	label.Text = title .. ": " .. _G.settings[key]
	label.TextColor3 = Color3.fromRGB(212, 212, 216)
	label.Font = Enum.Font.Gotham
	label.TextSize = 13
	label.TextXAlignment = Enum.TextXAlignment.Left
	label.Parent = frame
	local bar = Instance.new("Frame")
	bar.Size = UDim2.new(1, -32, 0, 8)
	bar.Position = UDim2.new(0, 16, 0, 36)
	bar.BackgroundColor3 = Color3.fromRGB(63, 63, 70)
	bar.Parent = frame
	local barCorner = Instance.new("UICorner", bar)
	barCorner.CornerRadius = UDim.new(0, 4)
	local fill = Instance.new("Frame")
	fill.Size = UDim2.new((_G.settings[key] - min) / (max - min), 0, 1, 0)
	fill.BackgroundColor3 = Color3.fromRGB(0, 255, 255)
	fill.Parent = bar
	local fillCorner = Instance.new("UICorner", fill)
	fillCorner.CornerRadius = UDim.new(0, 4)
	local sliderDragging = false
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			sliderDragging = true
			dragging = false
			local rel = math.clamp((input.Position.X - bar.AbsolutePosition.X) / bar.AbsoluteSize.X, 0, 1)
			local val = min + (max - min) * rel
			_G.settings[key] = step >= 1 and math.round(val) or math.floor(val * 10) / 10
			fill.Size = UDim2.new(rel, 0, 1, 0)
			label.Text = title .. ": " .. _G.settings[key]
		end
	end)
	UserInputService.InputChanged:Connect(function(input)
		if sliderDragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			local rel = math.clamp((input.Position.X - bar.AbsolutePosition.X) / bar.AbsoluteSize.X, 0, 1)
			local val = min + (max - min) * rel
			_G.settings[key] = step >= 1 and math.round(val) or math.floor(val * 10) / 10
			fill.Size = UDim2.new(rel, 0, 1, 0)
			label.Text = title .. ": " .. _G.settings[key]
		end
	end)
	UserInputService.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then sliderDragging = false end
	end)
end

local function createColorPicker(parent, title, key, order)
	local frame = Instance.new("Frame")
	frame.Size = UDim2.new(1, 0, 0, 48)
	frame.BackgroundColor3 = Color3.fromRGB(39, 39, 42)
	frame.LayoutOrder = order
	frame.Parent = parent
	local corner = Instance.new("UICorner", frame)
	corner.CornerRadius = UDim.new(0, 8)
	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(1, -80, 1, 0)
	label.Position = UDim2.new(0, 16, 0, 0)
	label.BackgroundTransparency = 1
	label.Text = title
	label.TextColor3 = Color3.fromRGB(212, 212, 216)
	label.Font = Enum.Font.Gotham
	label.TextSize = 14
	label.TextXAlignment = Enum.TextXAlignment.Left
	label.Parent = frame
	local colorPreview = Instance.new("Frame")
	colorPreview.Size = UDim2.fromOffset(32, 32)
	colorPreview.Position = UDim2.new(1, -48, 0.5, -16)
	colorPreview.BackgroundColor3 = _G.settings[key]
	colorPreview.Parent = frame
	local colorCorner = Instance.new("UICorner", colorPreview)
	colorCorner.CornerRadius = UDim.new(0, 6)
	local colors = {
		Color3.fromRGB(0, 255, 255),
		Color3.fromRGB(255, 0, 255),
		Color3.fromRGB(255, 255, 0),
		Color3.fromRGB(255, 0, 0),
		Color3.fromRGB(0, 255, 0),
		Color3.fromRGB(0, 0, 255),
	}
	local colorIndex = 1
	local btn = Instance.new("TextButton")
	btn.Size = UDim2.new(1,0,1,0)
	btn.BackgroundTransparency = 1
	btn.Text = ""
	btn.Parent = frame
	btn.MouseButton1Click:Connect(function()
		colorIndex = colorIndex % #colors + 1
		_G.settings[key] = colors[colorIndex]
		colorPreview.BackgroundColor3 = _G.settings[key]
	end)
end

local function createHeader(parent, text, order)
	local h = Instance.new("TextLabel")
	h.Size = UDim2.new(1, 0, 0, 16)
	h.BackgroundTransparency = 1
	h.Text = text
	h.TextColor3 = Color3.fromRGB(161, 161, 170)
	h.Font = Enum.Font.Gotham
	h.TextSize = 13
	h.TextXAlignment = Enum.TextXAlignment.Left
	h.LayoutOrder = order
	h.Parent = parent
end

local function clearContent()
	for _, child in ScrollFrame:GetChildren() do
		if child:IsA("Frame") or child:IsA("TextLabel") then child:Destroy() end
	end
end

function updateUI()
	clearContent()
	for id, btn in pairs(menuButtons) do
		if id == currentTab then
			btn.button.BackgroundColor3 = Color3.fromRGB(39, 39, 42)
			btn.label.TextColor3 = Color3.new(1,1,1)
		else
			btn.button.BackgroundColor3 = Color3.fromRGB(24, 24, 27)
			btn.label.TextColor3 = Color3.fromRGB(161, 161, 170)
		end
	end
	if currentTab == "Player" then
		createHeader(ScrollFrame, "REACH SETTINGS", 1)
		createToggle(ScrollFrame, "Reach Enabled", "reachOn", 2)
		createToggle(ScrollFrame, "Visualizer", "visOn", 3)
		createSlider(ScrollFrame, "Reach X", "reachX", 1, 150, 1, 4)
		createSlider(ScrollFrame, "Reach Y", "reachY", 1, 150, 1, 5)
		createSlider(ScrollFrame, "Reach Z", "reachZ", 1, 150, 1, 6)
		createSlider(ScrollFrame, "Offset X", "offsetX", -50, 50, 1, 7)
		createSlider(ScrollFrame, "Offset Y", "offsetY", -50, 50, 1, 8)
		createSlider(ScrollFrame, "Offset Z", "offsetZ", -50, 50, 1, 9)
		createSlider(ScrollFrame, "Box Opacity", "boxOpacity", 0, 1, 0.1, 10)
		createColorPicker(ScrollFrame, "Box Color", "boxColor", 11)
	elseif currentTab == "Movement" then
		createHeader(ScrollFrame, "MOVEMENT SETTINGS", 1)
		createToggle(ScrollFrame, "Infinite Stamina", "infStamOn", 2)
		createToggle(ScrollFrame, "Always Sprint", "alwaysSprintOn", 3)
