		createSlider(ScrollFrame, "Jump Power", "jumpPower", 0, 200, 1, 6)
	elseif currentTab == "Ball" then
		createHeader(ScrollFrame, "BALL SETTINGS", 1)
		createToggle(ScrollFrame, "Ball Predictor", "predOn", 2)
		createSlider(ScrollFrame, "Line Thickness", "lineThick", 0.2, 10, 0.1, 3)
		createToggle(ScrollFrame, "Ball Magnet", "bringBallOn", 4)
		createToggle(ScrollFrame, "Powershot", "powershotOn", 5)
		createToggle(ScrollFrame, "Infinite Charge", "infShotChargeOn", 6)
	elseif currentTab == "AutoGoal" then
		createHeader(ScrollFrame, "AUTO GOAL SETTINGS", 1)
		createToggle(ScrollFrame, "Auto Goal", "autoGoalFixed", 2)
		createToggle(ScrollFrame, "Auto Top Bins", "autoTopBinsOn", 3)
	elseif currentTab == "GKReach" then
		createHeader(ScrollFrame, "GK REACH SETTINGS", 1)
		createToggle(ScrollFrame, "Reach Enabled", "gkReachOn", 2)
